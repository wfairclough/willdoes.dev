<script>
	const canvas = document.querySelector('.starfield') as HTMLCanvasElement; 
	const context = canvas.getContext("2d")!;
	const stars = canvas.getBoundingClientRect().width;
	const colorrange = [0,15,60,200,240];
	function getRandom(min: number, max: number) {
    return Math.floor(Math.random() * (max - min + 1)) + min;
  }   
	for (var i = 0; i < stars; i++) {
    let x = Math.random() * canvas.offsetWidth;
    let y = Math.random() * canvas.offsetHeight;
    let radius = Math.random() * 1.4;
    let hue = colorrange[getRandom(0,colorrange.length - 1)];
    let sat = getRandom(50,100);
    context.beginPath();
    context.arc(x, y, radius, 0, 360);
    context.fillStyle = "hsl(" + hue + ", " + sat + "%, 88%)";
    context.fill();
	}
</script>


<style>
	.hero {
		position: relative;
		display: grid;
		align-items: center;
		justify-content: center;
		background: hsl(var(--bg-color-hsl));
		color: white;
		height: calc(100vh - var(--header-height));
		width: 100vw;
	}
	h1 {
		color: white;
		font-size: 12rem;
		z-index: 1;
		user-select: none;
		/* transform: skew(-70deg) translateX(2rem); */
	}
	h1:hover {
		animation: ludacrous-speed 1s ease-in-out 100ms infinite;
		transform-origin: bottom;
	}
	/* prefer no animations */
	@media (prefers-reduced-motion: reduce) {
		h1:hover {
			animation: none;
		}
	}


	.rocket {
		width: 20vw;
		height: 20vw;
	}
  @media (max-width: 720px) {
		h1 {
			font-size: 4rem;
		}	
	}
  @media (max-width: 1280px) {
		h1{
			font-size: 6rem;
		}	
	}
	@keyframes ludacrous-speed {
	  0%   {
			transform: skew(-50deg);
		}
	  20%  {
			transform: skew(-70deg);
		}
	  40%  {
			transform: skew(-40deg);
		}
	  60%  {
			transform: skew(-65deg);
		}
		80% {
			transform: skew(-30deg);
		}
	  100% {
			transform: skew(-50deg);
		}
	}
	.starfield {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background: transparent;
	}
</style>
